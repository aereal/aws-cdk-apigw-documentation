{
  "name": "aws-cdk-apigw-documentation",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "aereal <aereal@aereal.org>",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "@aereal/eslint-config/presets/docs-typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "createDefaultProgram": true,
      "project": "./tsconfig.json"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:ci": "rm -rf ./coverage/ && jest --collectCoverage --coverageDirectory ./coverage/",
    "build": "tsc -p .",
    "docs": "rm -rf docs/; typedoc ./src"
  },
  "devDependencies": {
    "@aereal/eslint-config": "^0.1.6",
    "@aws-cdk/assert": "^1.77.0",
    "@aws-cdk/assets": "^1.77.0",
    "@aws-cdk/aws-apigateway": "^1.77.0",
    "@aws-cdk/aws-applicationautoscaling": "^1.77.0",
    "@aws-cdk/aws-autoscaling-common": "^1.77.0",
    "@aws-cdk/aws-certificatemanager": "^1.77.0",
    "@aws-cdk/aws-cloudwatch": "^1.77.0",
    "@aws-cdk/aws-codeguruprofiler": "^1.77.0",
    "@aws-cdk/aws-ec2": "^1.77.0",
    "@aws-cdk/aws-efs": "^1.77.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.77.0",
    "@aws-cdk/aws-events": "^1.77.0",
    "@aws-cdk/aws-iam": "^1.77.0",
    "@aws-cdk/aws-kms": "^1.77.0",
    "@aws-cdk/aws-lambda": "^1.77.0",
    "@aws-cdk/aws-logs": "^1.77.0",
    "@aws-cdk/aws-route53": "^1.77.0",
    "@aws-cdk/aws-s3": "^1.77.0",
    "@aws-cdk/aws-s3-assets": "^1.77.0",
    "@aws-cdk/aws-sqs": "^1.77.0",
    "@aws-cdk/aws-ssm": "^1.77.0",
    "@aws-cdk/cloud-assembly-schema": "^1.77.0",
    "@aws-cdk/core": "^1.77.0",
    "@aws-cdk/cx-api": "^1.77.0",
    "@aws-cdk/region-info": "^1.77.0",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.17",
    "@types/node": "^14.14.11",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "constructs": "^3.0.4",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.2",
    "ts-jest": "^26.4.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^1.77.0",
    "@aws-cdk/core": "^1.77.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aereal/aws-cdk-apigw-documentation"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
